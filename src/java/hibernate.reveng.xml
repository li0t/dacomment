  <table-filter match-name="USUARIO"/>
  <table-filter match-name="DOCUMENTOS"/>
  <table-filter match-name="PERMISOS_PORTAFOLIO"/>
  <table-filter match-name="PERMISOS_DOCUMENTOS"/>
</hibernate-reverse-engineering>
